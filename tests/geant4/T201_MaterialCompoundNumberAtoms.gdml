<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="100"/>
		<constant name="wy" value="100"/>
		<constant name="wz" value="100"/>
		<constant name="bx" value="10"/>
		<constant name="by" value="10"/>
		<constant name="bz" value="10"/>
	</define>
	<materials>
		<element name="hydrogen" formula="H" Z="1">
			<atom value="1.01"/>
		</element>
		<element name="oxygen" formula="O" Z="8">
			<atom value="16.0"/>
		</element>
		<material name="water">
			<D value="1.0"/>
			<composite ref="hydrogen" n="2"/>
			<composite ref="oxygen" n="1"/>
		</material>
	</materials>
	<solids>
		<box name="ws" x="wx" y="wy" z="wz" lunit="mm"/>
		<box name="bs" x="bx" y="by" z="bz" lunit="mm"/>
	</solids>
	<structure>
		<volume name="bl">
			<materialref ref="water"/>
			<solidref ref="bs"/>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="b_pv1">
				<volumeref ref="bl"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
