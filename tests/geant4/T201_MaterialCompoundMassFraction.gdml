<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="100"/>
		<constant name="wy" value="100"/>
		<constant name="wz" value="100"/>
		<constant name="bx" value="10"/>
		<constant name="by" value="10"/>
		<constant name="bz" value="10"/>
	</define>
	<materials>
		<element name="nitrogen" formula="N" Z="7">
			<atom value="14.01"/>
		</element>
		<element name="oxygen" formula="O" Z="8">
			<atom value="16.0"/>
		</element>
		<material name="air">
			<D value="0.00129"/>
			<fraction ref="nitrogen" n="0.7"/>
			<fraction ref="oxygen" n="0.3"/>
		</material>
		<material name="iron" Z="26">
			<D value="7.874"/>
			<atom value="55.8452"/>
		</material>
	</materials>
	<solids>
		<box name="ws" x="wx" y="wy" z="wz" lunit="mm"/>
		<box name="bs" x="bx" y="by" z="bz" lunit="mm"/>
	</solids>
	<structure>
		<volume name="bl">
			<materialref ref="iron"/>
			<solidref ref="bs"/>
		</volume>
		<volume name="wl">
			<materialref ref="air"/>
			<solidref ref="ws"/>
			<physvol name="b_pv1">
				<volumeref ref="bl"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
