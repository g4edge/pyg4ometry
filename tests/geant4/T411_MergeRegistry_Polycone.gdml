<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx0" value="200"/>
		<constant name="wy0" value="200"/>
		<constant name="wz0" value="200"/>
		<constant name="bx" value="10"/>
		<constant name="by" value="10"/>
		<constant name="bz" value="10"/>
		<constant name="wx" value="100"/>
		<constant name="wy" value="100"/>
		<constant name="wz" value="100"/>
		<constant name="sphi" value="0"/>
		<constant name="dphi" value="1.5*pi"/>
		<constant name="z1" value="-10"/>
		<constant name="z2" value="0"/>
		<constant name="z3" value="10"/>
		<constant name="prmin1" value="1"/>
		<constant name="prmin2" value="5"/>
		<constant name="prmin3" value="3"/>
		<constant name="prmax1" value="9"/>
		<constant name="prmax2" value="9"/>
		<constant name="prmax3" value="5"/>
		<constant name="wx_1" value="100"/>
		<constant name="wy_1" value="100"/>
		<constant name="wz_1" value="100"/>
	</define>
	<materials/>
	<solids>
		<box name="ws" x="wx0" y="wy0" z="wz0" lunit="mm"/>
		<box name="bs" x="bx" y="by" z="bz" lunit="mm"/>
		<box name="ws_1" x="wx" y="wy" z="wz" lunit="mm"/>
		<polycone name="ps" startphi="sphi" deltaphi="dphi" lunit="mm" aunit="rad">
			<zplane rmin="prmin1" rmax="prmax1" z="z1"/>
			<zplane rmin="prmin2" rmax="prmax2" z="z2"/>
			<zplane rmin="prmin3" rmax="prmax3" z="z3"/>
		</polycone>
		<box name="ws_2" x="wx_1" y="wy_1" z="wz_1" lunit="mm"/>
	</solids>
	<structure>
		<volume name="bl">
			<materialref ref="G4_Au"/>
			<solidref ref="bs"/>
		</volume>
		<volume name="wl_1">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws_1"/>
			<physvol name="b_pv1">
				<volumeref ref="bl"/>
			</physvol>
		</volume>
		<volume name="pl">
			<materialref ref="G4_Fe"/>
			<solidref ref="ps"/>
		</volume>
		<volume name="wl_2">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws_2"/>
			<physvol name="p_pv1">
				<volumeref ref="pl"/>
			</physvol>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="l1_pv_1">
				<volumeref ref="wl_1"/>
				<position name="l1_pv_pos" x="-50.000000000000000" y="0.000000000000000" z="0.000000000000000" unit="mm"/>
			</physvol>
			<physvol name="l2_pv_1">
				<volumeref ref="wl_2"/>
				<position name="l2_pv_pos" x="50.000000000000000" y="0.000000000000000" z="0.000000000000000" unit="mm"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
