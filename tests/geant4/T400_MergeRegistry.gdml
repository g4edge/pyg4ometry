		<box name="ws_3" x="wx_2" y="wy_2" z="wz_2" lunit="mm"/>
	</solids>
	<structure>
		<volume name="bl">
			<materialref ref="air"/>
			<solidref ref="bs"/>
		</volume>
		<volume name="wl_1">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws_1"/>
			<physvol name="b_pv1">
				<volumeref ref="bl"/>
				<scaleref ref="sca_reflection"/>
			</physvol>
		</volume>
		<volume name="bl_1">
			<materialref ref="air_0"/>
			<solidref ref="bs_1"/>
		</volume>
		<volume name="wl_2">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws_2"/>
