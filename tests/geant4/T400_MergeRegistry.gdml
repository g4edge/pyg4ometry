				<volumeref ref="bl_2"/>
				<scaleref ref="sca_reflection_2"/>
			</physvol>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="l1_pv_1">
				<volumeref ref="wl_1"/>
