				<position name="l3_pv_pos" x="150.000000000000000" y="0.000000000000000" z="0.000000000000000" unit="mm"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
