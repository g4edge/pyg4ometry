<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="1000"/>
		<constant name="wy" value="1000"/>
		<constant name="wz" value="1000"/>
		<constant name="bx" value="100"/>
		<constant name="by" value="100"/>
		<constant name="bz" value="100"/>
		<constant name="mbx" value="800"/>
		<constant name="mby" value="100"/>
		<constant name="mbz" value="100"/>
	</define>
	<materials/>
	<solids>
		<box name="ws" x="wx" y="wy" z="wz" lunit="mm"/>
		<box name="bs" x="bx" y="by" z="bz" lunit="mm"/>
		<box name="mbs" x="mbx" y="mby" z="mbz" lunit="mm"/>
	</solids>
	<structure>
		<volume name="bl">
			<materialref ref="G4_Fe"/>
			<solidref ref="bs"/>
		</volume>
		<volume name="ml">
			<materialref ref="G4_Galactic"/>
			<solidref ref="mbs"/>
			<replicavol number="8">
				<volumeref ref="bl"/>
				<replicate_along_axis>
					<direction x="1"/>
					<width value="100.0" unit="mm"/>
					<offset value="0.0" unit="mm"/>
				</replicate_along_axis>
			</replicavol>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="ml_pv1">
				<volumeref ref="ml"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
